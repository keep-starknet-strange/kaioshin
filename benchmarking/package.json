{
  "name": "kaioshin-benchmarking",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Benchmarking of Kaioshin.",
  "main": "index.js",
  "scripts": {
    "test-chain": "artillery run -e local ./scripts/chain.yml",
    "test-execution": "artillery run -e local ./scripts/cairo-execution.yml --output report.json",
    "test-storage": "artillery run -e local ./scripts/cairo-storage.yml --output report-storage.json",
    "report": "artillery report report.json",
    "metrics": "node scripts/metrics.js",
    "full-test": "yarn test-storage && yarn metrics"
  },
  "keywords": [
    "kaioshin",
    "benchmark"
  ],
  "devDependencies": {
    "@polkadot/keyring": "^10.4.2",
    "artillery": "^2.0.0-30",
    "artillery-engine-substrate": "^1.0.6-2"
  }
}
